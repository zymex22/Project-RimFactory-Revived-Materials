<Defs>

	<!-- ===================================== Basic Plastic ======================================== -->
	<RecipeDef>
		<defName>PRF_MakeMixedPlasticOil</defName>
		<label>make plastic from oil</label>
		<description>Reform the hydrocarbons within oil to produce a solidified form that can be used in construction.</description>
		<jobString>Making plastic.</jobString>
		<allowMixingIngredients>false</allowMixingIngredients>
		<skillRequirements>
			<Crafting>3</Crafting>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>PRF_Oil</li>
					</thingDefs>
				</filter>
				<count>11</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>PRF_Oil</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<PRF_MixedPlastic>10</PRF_MixedPlastic>
		</products>
		<workAmount>600</workAmount>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.75</workSkillLearnFactor>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<recipeUsers>
			<li>PRF_MaterialsRefinery</li>
			<li>BiofuelRefinery</li>
		</recipeUsers>
		<researchPrerequisite>PRF_Research_BasicPlastics</researchPrerequisite>
	</RecipeDef>

