<Defs>
	<!-- ===================================== Carbon Weave/fabric ======================================== -->
	<RecipeDef>
		<defName>PRF_MakeCarbonFiber</defName>
		<label>make carbon fiber from carbon</label>
		<description>Synthesize carbon nanotubes and weave them into a fabric.</description>
		<jobString>Making carbon fiber.</jobString>
		<allowMixingIngredients>false</allowMixingIngredients>
		<skillRequirements>
			<Crafting>7</Crafting>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>PRF_Carbon</li>
					</thingDefs>
				</filter>
				<count>100</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>PRF_Carbon</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<PRF_CarbonFiber>10</PRF_CarbonFiber>
		</products>
		<workAmount>750</workAmount>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.75</workSkillLearnFactor>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<researchPrerequisite>PRF_Research_CarbonFiber</researchPrerequisite>
		<recipeUsers>
			<li>HandTailoringBench</li>
			<li>ElectricTailoringBench</li>
		</recipeUsers>
	</RecipeDef>

